{
  "name": "poc",
  "version": "0.0.1",
  "description": "Service to search the Salesforce demo Apparel API",
  "engines": {
    "node": "^14.0.0",
    "npm": "^6.14.4 || ^7.0.0 || ^8.0.0"
  },
  "repository": {
